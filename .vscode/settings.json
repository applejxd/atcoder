{
  "C_Cpp.default.cppStandard": "c++20",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    // `Run Code (Ctrl+Alt+N)` のコマンド
    // see https://atcoder.jp/contests/APG4b/rules?lang=ja
    // see https://github.com/formulahendry/vscode-code-runner
    "cpp": "cd $dir && g++-12 -std=gnu++20 -O2 -DONLINE_JUDGE -DATCODER -Wall -Wextra -mtune=native -march=native -fconstexpr-depth=2147483647 -fconstexpr-loop-limit=2147483647 -fconstexpr-ops-limit=2147483647 -I$workspaceRoot/third_party/ac-library -lboost_system -o a.out $fileName -lgmpxx -lgmp -I/usr/include/eigen3 && ./a.out"
  },
  "files.associations": {
    "dense": "cpp"
  },
}